{
  "name": "deploid",
  "version": "0.1.0",
  "type": "module",
  "description": "Build → package → sign → publish web apps to Android",
  "keywords": [
    "android",
    "capacitor",
    "pwa",
    "mobile",
    "build",
    "publish"
  ],
  "author": "MadsenDev",
  "license": "MIT",
  "homepage": "https://github.com/MadsenDev/shipwright",
  "repository": {
    "type": "git",
    "url": "https://github.com/MadsenDev/shipwright.git"
  },
  "bugs": {
    "url": "https://github.com/MadsenDev/shipwright/issues"
  },
  "private": false,
  "packageManager": "pnpm@9.12.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "publish": "pnpm -r publish"
  },
  "files": [
    "packages/cli/dist/",
    "packages/cli/bin/",
    "packages/core/dist/",
    "packages/plugins/*/dist/",
    "README.md",
    "docs/"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "typescript": "^5.9.3"
  }
}